{
  "lock": true,
  "nodeModulesDir": true,
  "imports": {
    "./": "./",
    "/": "./"
  },
  "tasks": {
    "dev": "deno run --watch --unstable -A main.ts --master=hono:123",
    "prod": "deno run --unstable -A main.ts --master=hono:123",
    "build": "deno compile --unstable -A --output docorch main.ts"
  },
  "lint": {
    "include": ["."],
    "exclude": ["node_modules/"],
    "rules": {
      "exclude": ["no-explicit-any", "ban-types"]
    }
  }
}
